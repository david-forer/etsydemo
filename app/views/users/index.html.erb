<h1> Our Breeders</h1>

<h3> All of our breeers are listed below.</h3>

<div class="center">
  <div align="center">
    <%= will_paginate %>
  </div>
    <div class="row">
        <% @users.each do |user| %>
        <% if user.Breeder %>
        <div class="col-md-3">
          <div class="thumbnail">
              <%= link_to image_tag(user.image.url), user %> 
              <div class="caption">
                <h3><%= user.name %></h3>
                <p><%= user.breeder_id %></p>
              </div>
              <% end %>
          </div>
        </div>
      <% end %>
    </div>
    <div align="center">
      <%= will_paginate %>
    </div>
</div>